&sdhc_1 {
	status = "okay";

	vmmc-supply = <&pm8226_l17>;
	vqmmc-supply = <&pm8226_l6>;

	bus-width = <8>;
	non-removable;

	pinctrl-names = "default";
	pinctrl-0 = <&sdhc1_pin_a>;
};

&sdhc_3 {
	status = "okay";

	max-frequency = <100000000>;
	non-removable;

	vmmc-supply = <&vreg_wlan>;
	vqmmc-supply = <&pm8226_l6>;

	#address-cells = <1>;
	#size-cells = <0>;

	bcrmf@1 {
		compatible = "brcm,bcm43430-fmac", "brcm,bcm4329-fmac";
		reg = <1>;

		brcm,drive-strength = <10>; // FIXME correct?

		interrupts-extended = <&tlmm 46 IRQ_TYPE_EDGE_FALLING>;
		interrupt-names = "host-wake";

		pinctrl-names = "default";
		pinctrl-0 = <&wlan_hostwake_pin>;
	};
};

&smbb {
	qcom,fast-charge-safe-current = <1500000>;
	qcom,fast-charge-current-limit = <350000>;
	qcom,fast-charge-safe-voltage = <4430000>;
	qcom,fast-charge-high-threshold-voltage = <4400000>;
	qcom,auto-recharge-threshold-voltage = <4300000>;
	qcom,minimum-input-voltage = <4400000>;
};

&tlmm {
	bluetooth_pins: bluetooth-pins {
		hostwake {
			pins = "gpio48";
			function = "gpio";
			drive-strength = <8>;
			bias-disable;
			input-enable;
		};

		devicewake {
			pins = "gpio61";
			function = "gpio";
			drive-strength = <8>;
			bias-disable;
			input-enable;
		};
	};

	blsp1_uart1_pin_a: blsp1-uart1-pin-a {
		pins = "gpio0", "gpio1", "gpio2", "gpio3";
		function = "blsp_uart1";
		drive-strength = <8>;
		bias-disable;
	};

	wlan_regulator_pin: wlan-regulator-pin {
		pins = "gpio35";
		function = "gpio";
		drive-strength = <2>;
		bias-pull-up;
	};

	wlan_hostwake_pin: wlan-hostwake-pin {
		pins = "gpio46";
		function = "gpio";
		drive-strength = <2>;
		bias-disable;
		input-enable;
	};
};

&usb {
	status = "okay";
	extcon = <&smbb>;
	dr_mode = "peripheral";
};

&usb_hs_phy {
	extcon = <&smbb>;
	v1p8-supply = <&pm8226_l10>;
	v3p3-supply = <&pm8226_l20>;
};
