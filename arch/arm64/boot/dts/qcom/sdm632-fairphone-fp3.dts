/dts-v1/;

#include "msm8953.dtsi"
#include "pm8953.dtsi"

/ {
	model = "Fairphone 3";
	compatible = "fairphone,fp3", "qcom,sdm632";
	qcom,msm-id = <349 0>;
	qcom,board-id = <8 3>;
	qcom,pmic-id = <0x010016 0x25 0x0 0x0>;
};

&usb3 {
	status = "okay";
};

&usb3_dwc3 {
	dr_mode = "peripheral";
};

&hsusb_phy {
	status = "okay";
};

&ssusb_phy {
	status = "okay";
};

&other_ext_mem {
	reg = <0x0 0x85b00000 0x0 0xd00000>;
};

